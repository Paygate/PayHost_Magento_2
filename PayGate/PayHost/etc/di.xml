<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="\PayGate\PayHost\Helper\Data">
        <arguments>
            <argument name="methodCodes" xsi:type="array">
                <item name="payhost" xsi:type="const">PayGate\PayHost\Model\Config::METHOD_CODE</item>
            </argument>
        </arguments>
    </type>
    <type name="\PayGate\PayHost\Helper\ArrayHelper">
        <arguments>
            <argument name="arrayHelper" xsi:type="object">PayGate\PayHost\Helper\ArrayHelper</argument>
        </arguments>
    </type>
</config>
